class Solution {
public:
    int countNegatives(vector<vector<int>>& grid) {
        int c=0;
        for(vector<int> x:grid){
            for(int i=0;i<x.size();i++){
                if(x[i]<0){
                    c=c+1;
                }
            }

        }
        return c;
        
    }
};
