class Solution {
public:
    int countEven(int num) {
        int c=0;
        for(int i=2;i<=num;i++){
            long int s=0;
            int n=i;
            while(n>0){
                s=s+(n%10);
                n=n/10;
            }
            if(s%2==0){
                c=c+1;

            }        
        }
        return c;
        
    }
};
