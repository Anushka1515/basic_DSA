class Solution {
public:
    vector<string> findRepeatedDnaSequences(string s) {
        unordered_map<string, int> hash;
        vector<string> ans;
        int start = 0;
        for(int i=0;i<s.size()-10+1;i++){
            if(s.size()-i+1<10){
                break;
            }
            hash[s.substr(i,10)]++;
        }
        for(auto x:hash){
            if(x.second>1){
                ans.push_back(x.first);
            }
        }
        return ans;
    }
};
