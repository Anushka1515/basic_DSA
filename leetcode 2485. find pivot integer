class Solution {
public:
    int pivotInteger(int n) {
        int sum=0;
        for(int i =1;i<=n;i++){
            sum+=i;
            int sum2=0;
            for(int j=i;j<=n;j++){
                sum2+=j;
            }
            if(sum==sum2){
                return i;
            }
            cout<<"sum="<<sum<<" ";
            cout<<"sum2="<<sum2<<endl;
            
        }
        return -1;
        
    }
};
        
