class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int e=0;
        int c=0;
        for(int i:nums){
            c=0;
            while(i){
                
                c=c+1;
                i=i/10;

            }
            if (c%2==0){
                e=e+1;

            }
        }
        return e;
    }
};
