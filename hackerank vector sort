#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    vector <int> v;
    
    for(int i;i<n;i++){
        int x;
        cin>>x;
        v.push_back(x);
        
    }
    sort(v.begin(),v.end());
    for(int j=0;j<v.size();j++){
        cout<<v[j]<<" ";
    }
    
  
    return 0;
}
