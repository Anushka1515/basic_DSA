class Solution {
public:
    int lengthOfLastWord(string s) {
        int c=0;
        string  x=s;
        for(int i=s.length()-1;i>=0;i--){
            if (x[i]!=' '){
                c=c+1;
            }
            else if(c!=0){
                break;
            }
        }
        return c;
        
        
    }
};
